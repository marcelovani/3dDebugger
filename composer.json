{
	"name": "marcelovani/3d_debugger",
	"description": "VR 3D Debugger",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Marcelo Vani",
			"email": "no@reply.com",
			"homepage": "https://www.marcelovani.eu",
			"role": "Developer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://packagist.drupal-composer.org"
		},
		{
			"type": "package",
			"package": {
				"name": "marcelovani/xhprof",
				"version": "dev-master",
				"source": {
					"url": "git@github.com:marcelovani/xhprof.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "marcelovani/Xhprof3D",
				"version": "dev-master",
				"source": {
					"url": "git@github.com:marcelovani/Xhprof3D.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	],
	"require": {
		"marcelovani/xhprof": "dev-master",
		"marcelovani/Xhprof3D": "dev-master",
		"mouf/nodejs-installer": "~1.0"
	},
	"require-dev": {
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"mouf": {
			"nodejs": {
				"version": "~0.12",
				"targetDir": "vendor/nodejs/nodejs",
				"forceLocal": false
			}
		}
	},
	"scripts": {
		"pre-install-cmd": [
		],
		"pre-update-cmd": [
		],
		"post-install-cmd": [
			"./vendor/nodejs/nodejs/bin/npm install"
		],
		"post-update-cmd": [
			"./vendor/nodejs/nodejs/bin/npm install"
		]
	}
}
